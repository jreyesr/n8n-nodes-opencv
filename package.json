    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/OpenCvNode/OpenCvNode.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "jimp": "^1.6.0"
